<?xml version="1.0" encoding="utf-8"?>
<ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:background="@drawable/bg_deco_gray"
    android:fillViewport="true" >
	<LinearLayout 
    android:id="@+id/record2_wdgz_linear1"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    android:orientation="vertical"
    android:background="@drawable/bg_deco_gray"
     >
	<RelativeLayout
	    android:id="@+id/record2_wdgz_notification_linear"
	    android:layout_width="fill_parent"
	    android:layout_height="45dip"
	    android:orientation="horizontal"
	    android:background="@drawable/main_navigation_highlight_bg" >

	    <Button
	        android:id="@+id/record2_wdgz_back"
	        android:layout_width="80dip"
	        android:layout_height="40dip"
	        android:layout_centerVertical="true"
	        android:layout_alignParentLeft="true"
	        android:layout_marginLeft="20dip"
	        android:textColor="@color/white"
	        android:text="返 回"
	        android:textSize="18dp" 
	        android:background="@drawable/zxbutton_selector"/>
		<TextView
		    android:layout_width="wrap_content"
		    android:layout_height="wrap_content"
		    android:layout_centerHorizontal="true"
		    android:layout_centerVertical="true"
		    android:textColor="#ffffff"
		    android:textSize="25dip"
		    android:text="网点改造"
		    />

	    <Button
	        android:id="@+id/record2_wdgz_save"
	        android:layout_width="80dip"
	        android:layout_height="40dip"
	        android:layout_centerVertical="true"
	        android:layout_alignParentRight="true"
	        android:layout_marginRight="20dip"
	        android:textColor="@color/white"
	        android:text="提 交"
	        android:textSize="18dip" 
	        android:background="@drawable/zxbutton_selector"
	        />
	</RelativeLayout>
	<LinearLayout 
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:orientation="vertical"
	    android:background="@drawable/textview_boder"
	    android:layout_marginLeft="10dip"
	    android:layout_marginTop="10dip"
	    android:layout_marginRight="10dip"
	    android:layout_marginBottom="5dip">
	    <TextView 
	        android:layout_width="fill_parent"
	        android:layout_height="match_parent"
	        android:text="网点改造"
	        android:textSize="25dip"
	        android:textColor="#000000"
	        android:drawableRight="@drawable/android_list_idex"
	        android:layout_marginRight="10dip"
	        android:layout_marginLeft="5dip" />	   
	         <ImageView android:layout_width="fill_parent"
         		android:layout_height="2dip"
         		android:layout_marginTop="5dip"
         		android:layout_marginBottom="5dip"
         		android:background="#9D9D9D"/> 
	    <LinearLayout 
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:orientation="horizontal"
	    android:gravity="center_vertical">
	    <TextView 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="业务编号:"
	        android:textColor="#000000"
	        android:textSize="20dip"
	        android:layout_marginRight="5dip"
	        android:layout_marginLeft="5dip"/>
	    <EditText 
	        android:layout_width="100dip"
	        android:layout_height="wrap_content"
	        android:layout_marginRight="10dip"
	        android:id="@+id/record2_wdgz_et_id"/>	
	        <TextView 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="查询业务编号"
	        android:textColor="#000000"
	        android:textSize="20dip"
	        android:layout_marginRight="5dip"
	        android:layout_marginLeft="5dip"/>
	    </LinearLayout>
	     <ImageView android:layout_width="fill_parent"
         		android:layout_height="2dip"
         		android:layout_marginTop="5dip"
         		android:layout_marginBottom="5dip"
         		android:background="#9D9D9D"/>
	    <LinearLayout 
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:orientation="horizontal"
	    android:gravity="center_vertical">
	    <TextView 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="总价格(元):"
	        android:textSize="20dip"
	        android:textColor="#000000"
	        android:layout_marginRight="5dip"
	        android:layout_marginLeft="5dip"/>
	    <EditText 
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginRight="10dip"
	        android:id="@+id/record2_wdgz_et_price"/>	
	    </LinearLayout>
	    <ImageView android:layout_width="fill_parent"
         		android:layout_height="2dip"
         		android:layout_marginTop="5dip"
         		android:layout_marginBottom="5dip"
         		android:background="#9D9D9D"/>
	<LinearLayout 
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:orientation="horizontal"
	    android:gravity="center_vertical">
	    <TextView 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="测量日期:"
	        android:textSize="20dip"
	        android:textColor="#000000"
	        android:layout_marginRight="5dip"
	        android:layout_marginLeft="5dip"/>
	    <EditText 
	        android:layout_width="200dip"
	        android:layout_height="wrap_content"
	        android:layout_marginRight="10dip"
	        android:id="@+id/record2_wdgz_et_date"/>	
	        <Button
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:text="日期"
	        android:id="@+id/record2_wdgz_date"/>
	    </LinearLayout>
	    <ImageView android:layout_width="fill_parent"
         		android:layout_height="2dip"
         		android:layout_marginTop="5dip"
         		android:layout_marginBottom="5dip"
         		android:background="#9D9D9D"/>
	    <LinearLayout 
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:orientation="horizontal"
	    android:gravity="center_vertical">
	    <TextView 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="备        注:"
	        android:textSize="20dip"
	        android:textColor="#000000"
	        android:layout_marginRight="5dip"
	        android:layout_marginLeft="5dip"/>
	    <EditText 
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:layout_marginRight="10dip"
	        android:id="@+id/record2_wdgz_et_remarks"/>	
	    </LinearLayout>
	 <ImageView android:layout_width="fill_parent"
         		android:layout_height="2dip"
         		android:layout_marginTop="5dip"
         		android:layout_marginBottom="5dip"
         		android:background="#9D9D9D"/>
	    <LinearLayout 
	    android:layout_width="fill_parent"
	    android:layout_height="wrap_content"
	    android:orientation="horizontal"
	    android:gravity="center_vertical"
	    android:layout_marginBottom="5dip">
	    <TextView 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:text="原表附件:"
	        android:textSize="20dip"
	        android:textColor="#000000"
	        android:layout_marginRight="5dip"
	        android:layout_marginLeft="5dip"/>
	    <ImageView 
	        android:layout_width="30dip"
	        android:layout_height="30dip"
	        android:background="@drawable/add1"
	        android:id="@+id/record2_wdgz_iv_annex"/>
	     <ImageView 
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:id="@+id/record2_wdgz_iv2_annex"/>	
	    </LinearLayout>
	</LinearLayout>
	</LinearLayout>
</ScrollView>